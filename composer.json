{
    "name": "tu-nombre/mi-plugin",
    "description": "Plugin WordPress avanzado con Symfony Components y React",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
      {
        "name": "Luis Felipe Fidencio Chavez",
        "email": "lfelipe_ch30@hotmail.com"
      }
    ],
    "require": {
      "php": ">=8.2",
      "doctrine/migrations": "^3.6",             
      "doctrine/orm": "^2.17",                    
      "symfony/console": "^7.0",
      "symfony/dependency-injection": "^7.0",
      "symfony/http-foundation": "^7.0",
      "symfony/routing": "^7.0",
      "symfony/workflow": "^7.0",
      "psr/container": "^2.0"

    },
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      }
    },
    "config": {
      "optimize-autoloader": true,
      "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
  }
  